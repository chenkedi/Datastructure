package leetcode;

public class PowerOf3{
	public static void main(String[] args){
		System.out.println(powerOf3(27));
	}

	private static boolean powerOf3(int n) {
		if(n<3 && n!=0)
			return false;
		for(;n>2;n/=3){
            if(n%3!=0)
                return false;
        }
        
        return true;
		
	}

}
